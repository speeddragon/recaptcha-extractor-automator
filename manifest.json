{
  "manifest_version": 2,
  "name": "ReCaptcha Extractor and Automator",
  "description": "Solve, extract and automate ReCaptcha data collection.",
  "version": "1.0.8",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "*://*/",
    "webRequest",
    "webRequestBlocking",
    "activeTab",
    "tabs",
    "clipboardRead",
    "clipboardWrite",
    "storage",
    "contextMenus",
    "notifications"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon_96.png",
    "default_title": "ReCaptcha Extractor and Automator"
  },
  "web_accessible_resources": ["copy_to_clipboard.js", "replace_recaptcha.js"]
}
